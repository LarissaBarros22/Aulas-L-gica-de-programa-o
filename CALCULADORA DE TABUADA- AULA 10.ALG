algoritmo "tabuada (enquanto)"
var
    n, resul, i : inteiro
inicio
      escreva ("escolha o numero pra fazer a tabuada dele: ")
      leia (n)
      i <- 0
      Enquanto ( i < 10 ) FACA
               i <- i+1
               resul <- n * i
               escreval (n , " X ", i, " = ", resul)

      FimEnquanto
fimalgoritmo